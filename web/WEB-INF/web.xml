<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>QuestionsUpload</servlet-name>
        <servlet-class>processors.QuestionsUpload</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>QuestionDelete</servlet-name>
        <servlet-class>processors.QuestionDelete</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>QuestionEdit</servlet-name>
        <servlet-class>processors.QuestionEdit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TopicDelete</servlet-name>
        <servlet-class>processors.TopicDelete</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TopicAdd</servlet-name>
        <servlet-class>processors.TopicAdd</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TestAdd</servlet-name>
        <servlet-class>processors.TestAdd</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentAdd</servlet-name>
        <servlet-class>processors.StudentAdd</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>QuestionsUpload</servlet-name>
        <url-pattern>/QuestionsUpload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QuestionDelete</servlet-name>
        <url-pattern>/QuestionDelete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QuestionEdit</servlet-name>
        <url-pattern>/QuestionEdit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TopicDelete</servlet-name>
        <url-pattern>/TopicDelete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TopicAdd</servlet-name>
        <url-pattern>/TopicAdd</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TestAdd</servlet-name>
        <url-pattern>/TestAdd</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentAdd</servlet-name>
        <url-pattern>/StudentAdd</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
